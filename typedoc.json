{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "src/types/index.ts",
    "src/lib/utils.ts",
    "src/lib/fonts.ts",
    "src/env.ts",
    "src/config/site.ts",
    "src/config/projects.ts",
    "src/config/skills.ts",
    "src/config/navigation.ts",
    "src/i18n/locales.ts",
    "src/i18n/navigation.ts",
    "src/i18n/request.ts",
    "src/i18n/routing.ts",
    "src/middleware.ts",
    "src/components/shared/animated-wrapper.tsx",
    "src/components/shared/project-card.tsx",
    "src/components/shared/section-heading.tsx",
    "src/components/shared/json-ld.tsx",
    "src/components/layout/header.tsx",
    "src/components/layout/footer.tsx",
    "src/components/layout/theme-toggle.tsx",
    "src/components/layout/mobile-nav.tsx",
    "src/components/layout/locale-switcher.tsx",
    "src/components/sections/hero.tsx",
    "src/components/sections/about-preview.tsx",
    "src/components/sections/projects-showcase.tsx",
    "src/components/sections/skills.tsx",
    "src/components/sections/contact-cta.tsx",
    "src/app/sitemap.ts",
    "src/app/robots.ts"
  ],
  "out": "docs/api",
  "tsconfig": "./tsconfig.json",
  "exclude": [
    "**/*.test.tsx",
    "**/*.test.ts",
    "**/*.a11y.test.tsx",
    "**/node_modules/**",
    "**/.next/**",
    "src/components/ui/**"
  ],
  "name": "Portfolio API Documentation",
  "readme": "none",
  "excludePrivate": true,
  "excludeInternal": true,
  "includeVersion": false,
  "categorizeByGroup": true,
  "sort": ["source-order"],
  "navigationLinks": {
    "Portfolio": "https://github.com/andre/portfolio"
  }
}
